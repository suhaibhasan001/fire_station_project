            <div class="sidenav-header">
                <a class="navbar-brand px-4 py-3 m-0" href="https://www.piac.com.pk/">
                    <img src="../assets/download.png" class="navbar-brand-img" width="26" height="26" alt="main_logo">
                    <span class="ms-1 text-sm text-dark">PIA</span>
                </a>
            </div>
            <hr class="horizontal dark mt-0 mb-2">
            <div class="collapse navbar-collapse w-auto" id="sidenav-collapse-main">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active bg-gradient-dark text-white" runat="server" href="dashboard.aspx">
                            <i class="material-symbols-rounded opacity-5">dashboard</i>
                            <span class="nav-link-text ms-1">Dashboard</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" runat="server" href="training.aspx">
                            <i class="material-symbols-rounded opacity-5">table_view</i>
                            <span class="nav-link-text ms-1">Safety Training</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="Evacuationdrills.aspx">
                            <i class="material-symbols-rounded opacity-5">local_fire_department</i>
                            <span class="nav-link-text ms-1">Evacuation Drills</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="fireaccidents.aspx">
                            <i class="material-symbols-rounded opacity-5">receipt_long</i>
                            <span class="nav-link-text ms-1">Fire Accidents</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="extinguishers.aspx">
                            <i class="material-symbols-rounded opacity-5">view_in_ar</i>
                            <span class="nav-link-text ms-1">Extinguishers</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="Alarmsystem.aspx">
                            <i class="material-symbols-rounded opacity-5">format_textdirection_r_to_l</i>
                            <span class="nav-link-text ms-1">Alarm System</span>
                        </a>
                    </li>
                    
                    
        <li class="nav-item mt-3">
          <h6 class="ps-4 ms-2 text-uppercase text-xs text-dark font-weight-bolder opacity-5">Account pages</h6>
        
        <li class="nav-item">
          <a class="nav-link text-dark" href="Login.aspx">
            <i class="material-symbols-rounded opacity-5">login</i>
            <span class="nav-link-text ms-1">Sign In</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-dark" href="login.aspx">
            <i class="material-symbols-rounded opacity-5">assignment</i>
            <span class="nav-link-text ms-1">Sign Up</span>
          </a>
        </li>
                </ul>
            </div>
        </aside>

        <!-- Main Content -->
        <main class="main-content position-relative h-100 border-radius-lg">
            <asp:ContentPlaceHolder ID="MainContent" runat="server" />
        </main>
    </form>

    <!-- JS -->
    <script src="assets/js/core/bootstrap.min.js"></script>
    <script src="assets/js/material-dashboard.min.js?v=3.2.0"></script>
</body>
</html>
